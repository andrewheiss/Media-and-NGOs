if($('#popup ins').length){
	$(window).scroll(function(){
		// get the height of #container
		var h = $("#container").height();
		//the vertical scroll position 
		var y = $(window).scrollTop();
		//checking if the vertical scroll position is 65% from the hight of the page
		if(y > (h*.65)){
			// the transion is done through CSS
			$("#popup").removeClass("closed").addClass("open");
			return;
		}
